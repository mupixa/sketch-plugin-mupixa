{
  "name": "mupixa",
  "description": "Mupixa allows you to leverage the full potential of your prototypes by conducting usability research fast and effortlessly.",
  "version": "1.1.1",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Mupixa",
    "manifest": "src/manifest.json",
    "main": "mupixa.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mupixa/sketch-plugin-mupixa.git"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.5"
  },
  "author": "www.mupixa.com",
  "dependencies": {
    "sketch-module-web-view": "^3.0.1",
    "sketch-polyfill-fetch": "github:skpm/sketch-polyfill-fetch",
    "skpm": "^1.2.0"
  }
}
